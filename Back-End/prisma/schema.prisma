generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model SystemUser {
  id       Int    @id @default(autoincrement())
  name     String @map("nome")
  userName String @unique @map("usuario")
  password String @map("senha")

  @@map("usuarios")
}
